body{background-color:#fff;background:fixed -webkit-gradient( linear,left bottom,right top,from(rgb(100,29,141)),color-stop(40%,rgba(30,89,177,1)),to(rgba(0,255,160,1)) );background:fixed linear-gradient( to top right,rgb(100,29,141) 0%,rgba(30,89,177,1) 40%,rgba(0,255,160,1) 100% )}#main{color:#fff;background:rgba(255,255,255,.25);-webkit-box-shadow:0 8px 32px 0 rgba(31,38,135,.37);box-shadow:0 8px 32px 0 rgba(31,38,135,.37);backdrop-filter:blur(4px);-webkit-backdrop-filter:blur(4px);border-radius:10px;border:1px solid rgba(255,255,255,.18)}#main.a{background:rgba(0,0,0,.25);-webkit-box-shadow:0 8px 32px 0 rgba(31,38,135,.37);box-shadow:0 8px 32px 0 rgba(31,38,135,.37);backdrop-filter:blur(4px);-webkit-backdrop-filter:blur(4px);border-radius:10px;border:1px solid rgba(255,255,255,.18)}